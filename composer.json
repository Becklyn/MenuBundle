{
    "name": "becklyn/menu",
    "type": "symfony-bundle",
    "description": "Menu bundle for generating and rendering menus in Symfony.",
    "homepage": "https://github.com/Becklyn/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.1",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/expression-language": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/routing": "^4.3 || ^5.0",
        "symfony/security-core": "^4.3 || ^5.0",
        "symfony/translation-contracts": "^1.1 || ^2.0",
        "twig/twig": "^2.11 || ^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-next": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Menu\\": "tests/"
        }
    }
}
