{
    "name": "becklyn/menu",
    "type": "symfony-bundle",
    "description": "Menu bundle for generating and rendering menus in Symfony.",
    "homepage": "https://github.com/Becklyn/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/log": "^1.1.4",
        "symfony/config": "^5.4.3 || ^6.0.3",
        "symfony/dependency-injection": "^5.4.3 || ^6.0.3",
        "symfony/expression-language": "^5.4.3 || ^6.0.3",
        "symfony/http-kernel": "^5.4.4 || ^6.0.4",
        "symfony/routing": "^5.4.3 || ^6.0.3",
        "symfony/security-core": "^5.4.3 || ^6.0.3",
        "symfony/translation-contracts": "^1.1.0 || ^2.0.0 || ^3.0.0",
        "twig/twig": "^3.3.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/css-selector": "^5.4.3 || ^6.0.3",
        "symfony/dom-crawler": "^5.4.3 || ^6.0.3",
        "symfony/phpunit-bridge": "^5.4.3 || ^6.0.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-next": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Menu\\": "tests/"
        }
    }
}
